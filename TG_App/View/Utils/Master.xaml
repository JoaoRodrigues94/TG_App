<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:cont="clr-namespace:TG_App.View"
             x:Class="TG_App.View.Utils.Master">
  <MasterDetailPage.Master>
    <ContentPage Title="Menu">
      <StackLayout BackgroundColor="LightBlue">
                <Button Text="Início" BackgroundColor="White" Clicked="GoHome"/>
                <Button Text="Alimentos" BackgroundColor="White" Clicked="Alimentos"/>
                <Button Text="Exames De Glicemia" BackgroundColor="White" Clicked="Exames"/>
                <Button Text="Relatórios" BackgroundColor="White" Clicked="GoRelatorio"/>
                <Button Text="Atividades Físicas" BackgroundColor="White" Clicked="AtFisico"/>
                <Button Text="Agenda" BackgroundColor="White" Clicked="Agenda"/>
                <Button Text="Configurações" BackgroundColor="White" Clicked="Configuracoes"/>
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <cont:HomePage/>
  </MasterDetailPage.Detail>
</MasterDetailPage>